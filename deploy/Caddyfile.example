{
  email you@example.com
}

hackorum.example.com {
  reverse_proxy web:3000 {
    transport http {
      response_header_timeout 30s
    }
  }
  log {
    output stdout
  }
}

umami.hackorum.dev {
  reverse_proxy umami:3000
  log {
    output stdout
  }
}

dumps.hackorum.dev {
  root * /dumps/public
  file_server browse
  log {
    output stdout
  }
}
